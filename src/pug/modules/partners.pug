    var partnersSettings = {
        slidesPerView: 'auto',
        loop: true,
        pagination: {
            el: ".partners__pagination",
            dynamicBullets: true,
            dynamicMainBullets: 3,
            clickable: true
        },
        breakpoints: {
            580: {
                slidesPerView: 7
            }
        },
    }
section.partners
